<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel ui:field="body" addStyleNames="centralizaBody">
            <g:HorizontalPanel>
		        <g:VerticalPanel addStyleNames="colunaEsquerda" >
                	<g:Label addStyleNames="label" text="  Id: "         ></g:Label>
					<g:Label addStyleNames="label" text="* Login: "        ></g:Label>
					<g:Label addStyleNames="label" text="* Senha: " ></g:Label>
					<g:Label addStyleNames="label" text="* Confirmação da Senha: " ></g:Label>
					<g:Label addStyleNames="label" text="* Nome: "         ></g:Label>
					<g:Label addStyleNames="label" text="* Registro na empresa: "></g:Label>
					<g:Label addStyleNames="label" text="* Cargo: "        ></g:Label>
					<g:Label addStyleNames="label" text="* Departamento: " ></g:Label>
					<g:Label addStyleNames="label" text="* Aministrador: " ></g:Label>
				</g:VerticalPanel>
		        <g:VerticalPanel addStyleNames="colunaDireita" >
                	<g:TextBox ui:field="id"></g:TextBox>
                	<g:TextBox ui:field="login"></g:TextBox>
                	<g:PasswordTextBox ui:field="senha"></g:PasswordTextBox>
                	<g:PasswordTextBox ui:field="confirmacaoSenha"></g:PasswordTextBox>
                	<g:TextBox ui:field="nome"></g:TextBox>
					<g:TextBox ui:field="identificacao"></g:TextBox>
					<g:TextBox ui:field="cargo"></g:TextBox>
					<g:TextBox ui:field="departamento"></g:TextBox>
					<g:CheckBox ui:field="admin"></g:CheckBox>
				</g:VerticalPanel>
			</g:HorizontalPanel>
			<g:Label text="* Campos obrigatórios"></g:Label>
            <g:HorizontalPanel>
                <g:Button ui:field="botaoSalvar"   text="Salvar"></g:Button>
                <g:Button ui:field="botaoCancelar" text="Cancelar"></g:Button>
            </g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 